@article {delJunco_2018,
	author = {del Junco, Clara and Tociu, Laura and Vaikuntanathan, Suriyanarayanan},
	title = {Energy dissipation and fluctuations in a driven liquid},
	volume = {115},
	number = {14},
	pages = {3569--3574},
	year = {2018},
	doi = {10.1073/pnas.1713573115},
	publisher = {National Academy of Sciences},
	abstract = {Characterizing how energy consumption changes the properties of model nonequilibrium systems, particularly those with time-dependent steady states, remains an open problem{\textemdash}one that is key to advancing our understanding of a wide range of experimental and biological systems. Here, we derive a relation between the work done on a system of particles driven by time-periodic forces and the force fluctuations in the system. This relation gives us information about how pushing the system out of equilibrium modifies its structure and transport properties and leads to phase separation. Our results suggest ways to explore how energy dissipation can be used to tune the properties of nonequilibrium materials.Minimal models of active and driven particles have recently been used to elucidate many properties of nonequilibrium systems. However, the relation between energy consumption and changes in the structure and transport properties of these nonequilibrium materials remains to be explored. We explore this relation in a minimal model of a driven liquid that settles into a time periodic steady state. Using concepts from stochastic thermodynamics and liquid state theories, we show how the work performed on the system by various nonconservative, time-dependent forces{\textemdash}this quantifies a violation of time reversal symmetry{\textemdash}modifies the structural, transport, and phase transition properties of the driven liquid.},
	issn = {0027-8424},
	URL = {https://www.pnas.org/content/115/14/3569},
	eprint = {https://www.pnas.org/content/115/14/3569.full.pdf},
	journal = {Proceedings of the National Academy of Sciences}
}


